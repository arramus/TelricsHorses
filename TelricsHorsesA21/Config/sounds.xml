<configs>
<!-- sets bounce sounds -->
<set xpath="/Sounds/SoundDataNode[@name='vwheel_bounce']/AudioClip/@ClipName">Sounds/Misc/silencefiller</set>


<append xpath="/Sounds">
<SoundDataNode name="SoundEffectHorseNeigh">
<AudioSource name="Sounds/AudioSource_WeaponFire"/>
<NetworkAudioSource name="Sounds/AudioSource_WeaponFire_Network"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorsesA21.unity3d?SoundEffectHorseNeigh1"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorsesA21.unity3d?SoundEffectHorseNeigh2"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorsesA21.unity3d?SoundEffectHorseNeigh3"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorsesA21.unity3d?SoundEffectHorseNeigh4"/>
	<Priority name="3"/>
	<maxVoicesPerEntity value="10"/>
  <LowestPitch name="1"/>
  <HighestPitch name="1.05"/>
	<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value=".1"/> 
</SoundDataNode>
<SoundDataNode name="SoundEffectHorseGallop">
<AudioSource name="Sounds/AudioSource_WeaponFire"/>
<NetworkAudioSource name="Sounds/AudioSource_WeaponFire_Network"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorsesA21.unity3d?SoundEffectHorseGallop" Loop="true"/>
	<Priority name="3"/>
	<maxVoicesPerEntity value="10"/>
  <LowestPitch name="1"/>
  <HighestPitch name="1.05"/>
	<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value=".1"/> 
</SoundDataNode>
<SoundDataNode name="SoundEffectHorseGallopSingle">
<AudioSource name="Sounds/AudioSource_WeaponFire"/>
<NetworkAudioSource name="Sounds/AudioSource_WeaponFire_Network"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorsesA21.unity3d?SoundEffectHorseGallopSingle"/>
	<Priority name="3"/>
	<maxVoicesPerEntity value="10"/>
  <LowestPitch name="1"/>
  <HighestPitch name="1.05"/>
	<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="50"/> <MaxRepeatRate value=".45"/> 
</SoundDataNode>
<SoundDataNode name="SoundEffectHorseWalkSingle">
<AudioSource name="Sounds/AudioSource_WeaponFire"/>
<NetworkAudioSource name="Sounds/AudioSource_WeaponFire_Network"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorsesA21.unity3d?SoundEffectHorseWalk1"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorsesA21.unity3d?SoundEffectHorseWalk2"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorsesA21.unity3d?SoundEffectHorseWalk3"/>
	<Priority name="3"/>
	<maxVoicesPerEntity value="10"/>
  <LowestPitch name="1"/>
  <HighestPitch name="1.05"/>
	<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="50"/> <MaxRepeatRate value=".27"/> 
</SoundDataNode>

		<!-- Horse Idle Snort -->
<SoundDataNode name="horseIdle"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
	<AudioClip ClipName="#@modfolder:Resources/TelricsHorseIdleA21.unity3d?HorseIdal" Loop="true"/>
	<LocalCrouchVolumeScale value="1.0"/> <CrouchNoiseScale value="0.5"/> <NoiseScale value="1"/> <MaxVoices value="10"/> <MaxRepeatRate value="0.01"/> 
</SoundDataNode>

</append>


</configs>