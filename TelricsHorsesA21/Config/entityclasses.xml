<configs>

	<append xpath="/entity_classes/entity_class[@name='playerMale']">
		<effect_group>	
			<triggered_effect trigger="onSelfEnteredGame" action="AddBuff" target="self" buff="buffHorseMovementPlayer"/>
			<triggered_effect trigger="onSelfRespawn" action="AddBuff" target="self" buff="buffHorseMovementPlayer"/>
		</effect_group>
	</append>

<append xpath="/entity_classes">
<entity_class name="vehicleLightBrownHorse">
	<property name="Tags" value="vehicle"/>
	<property name="Class" value="EntityVJeep"/>
	<property name="Parent" value="Cars"/>
	<property name="Prefab" value="#@modfolder:Resources/TelricsHorsesA21.unity3d?MountLightBrownHorseIdlePrefab"/>
	<property name="ModelType" value="Standard"/>
	<property name="SurfaceCategory" value="organic"/>
	<property name="IgnoreTrigger" value="true"/>
	<property name="LootListAlive" value="vehicleMinibike"/>
	<property name="IsEnemyEntity" value="false"/>
	<property name="Explosion.ParticleIndex" value="0"/>
	<property name="Explosion.RadiusBlocks" value="5"/>
	<property name="Explosion.RadiusEntities" value="5"/>
	<property name="Explosion.BlockDamage" value="1"/>
	<property name="Explosion.EntityDamage" value="1"/>
	<property name="RotateToGround" value="true"/>
	<property name="Faction" value="none"/>
	<property name="MapIcon" value="ui_game_symbol_telrichorse"/>
	<property name="NavObject" value="TelricHorse" />
	<property name="Buffs" value="buffHorseMovement"/>
</entity_class>
